<!--index.wxml-->
<block wx:if="{{$taroCompReady}}">
  <View>
    <Text className="text">{{text}}</Text>
  </View>
  <view>
    <View class="container">
    <View class="conut">
      <Text>count: {{count}}</Text>
    </View>
    <Button bindtap="onAddClick" class="btn">+1</Button>
    <Button bindtap="onReduceClick" class="btn">-1</Button></View>
  </view>
</block>

